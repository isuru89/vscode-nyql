{
  "name": "vscode-nyql",
  "displayName": "NyQL",
  "description": "NyQL Extension for Visual Studio Code",
  "version": "0.0.1",
  "publisher": "nyql",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Languages"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "nyql",
        "aliases": [
          "NyQL",
          "nyql"
        ],
        "extensions": [
          ".nyql",
          ".groovy"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nyql",
        "scopeName": "source.nyql",
        "path": "./syntaxes/nyql.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "nyql",
        "path": "./snippets/nyql.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "tslint": "tslint -c tslint.json src/extension.ts"
  },
  "devDependencies": {
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^6.0.40",
    "typescript": "^2.1.4",
    "vscode": "^1.1.10"
  },
  "dependencies": {
    "mysql2": "^1.5.3"
  }
}
